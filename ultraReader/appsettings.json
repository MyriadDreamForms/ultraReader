{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=ultrareader;Username=postgres;Password=postgres",
    "RedisConnection": "redis:6379"
  },
  "Caching": {
    "EnableRedisCache": true,
    "DefaultCacheTime": 60
  },
  "Security": {
    "RequireEmailConfirmation": false,
    "LockoutMinutes": 10,
    "MaxFailedLoginAttempts": 5,
    "AutoLockoutEnabled": true
  },
  "FileStorage": {
    "MaxUploadSize": 10485760,
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".webp"]
  }
} 